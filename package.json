{
  "name": "furry-eureka",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --open --hot",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "new": "plop --plopfile generators/index.js",
    "fontello:open": "fontello-cli --config www/assets/fontello/config.json open",
    "fontello:install": "fontello-cli --config www/assets/fontello/config.json install",
    "build": "webpack --mode production",
    "init": "npm install && npm run build:dev:vendors",
    "dev": "better-npm-run dev",
    "build:production:vendors": "better-npm-run build:production:vendors",
    "build:production:client": "better-npm-run build:production:client",
    "build:production": "better-npm-run build:production",
    "build:client": "better-npm-run build:client",
    "watch:ssr": "better-npm-run watch:ssr",
    "start:dev": "NODE_PATH=./dist CFG_DIR=$(pwd)/dist/configs node dist/server/index.js",
    "start:prod": "cd /var/www/html && APP_VERSION=$(cat $(pwd)/.version) CFG_DIR=$(pwd)/dist/configs/ NODE_PATH=$(pwd)/dist node dist/server/index.js",
    "build:configs": "tsc --project configs",
    "build:server": "tsc --project server",
    "build:dev:vendors": "NODE_ENV=development webpack --mode=development --config webpack/config/vendors.config.ts",
    "build:ssr": "webpack --mode=production --config webpack/config/ssr.config.ts",
    "clean": "rm -rf dist && mkdir dist"
  },
  "betterScripts": {
    "dev": {
      "command": "npm-run-all build:configs build:server && npm-run-all --parallel --print-name watch:ssr start:dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:production": {
      "command": "npm-run-all build:configs build:server build:production:vendors build:ssr build:production:client",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:production:vendors": {
      "command": "webpack --mode=production --config webpack/config/vendors.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:production:client": {
      "command": "webpack --mode=production --config webpack/config/client.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "watch:ssr": {
      "command": "webpack --watch --config webpack/config/ssr.config.ts",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "browserslist": [
    "> 1%",
    "ie > 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "bash -c tsc",
      "jest --passWithNoTests"
    ],
    "*.css": "stylelint --fix --allow-empty-input"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/athens-praktikum-yandex/furry-eureka.git"
  },
